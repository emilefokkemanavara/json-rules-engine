{
    "compilerOptions": {
      "target": "ESNext" ,
      "module": "ESNext",
      "moduleResolution": "Node",
  
      "esModuleInterop": true,
      
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "noEmit": true,
      "noImplicitAny": false
    }
