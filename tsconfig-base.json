{
    "compilerOptions": {
        "target": "ESNext" ,
        "module": "ESNext",
        "moduleResolution": "Node",
        "composite": true,
        "esModuleInterop": true,
        
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "emitDeclarationOnly": true,
        "outDir": "dist"
    }
}